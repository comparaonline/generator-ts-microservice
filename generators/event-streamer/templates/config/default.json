{
  "kafka": {
    "global": {
      "kafkaHost": "localhost:9092"
    },
    "consumer": {
      "groupId": "kafka_topic",
      "topics": [
        "kafka_topic"
      ],
      "backpressure": {
        "pause": 10,
        "resume": 5
      }
    },
    "producer": {
      "defaultTopic": "kafka_topic"
    }
  }
}
